{
  "include": ["src/**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "target": "ES2019",

    "module": "ES2020",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "rootDir": ".",
    "outDir": "./dist",
  }
}
